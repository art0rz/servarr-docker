{
  "name": "vpn-qbit-health",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dist/server/health-server.js",
    "dev:server": "tsup --watch",
    "dev:client": "vite",
    "build:server": "tsup",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit && tsc --project tsconfig.client.json --noEmit",
    "check": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "dockerode": "^4.0.9",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/chart.js": "^2.9.41",
    "@types/dockerode": "^3.3.46",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2"
  }
}
